<?xml version="1.0" encoding="utf-8" ?>
<Sprites>
    <koseiHelper_goomba path="objects/KoseiHelper/Goomba/" start="idle">
        <Justify x="0.5" y="0.5" />
        <Loop id="idle" path="idle" delay="0.1" frames="00" />
		<Loop id="walk" path="walk" delay="0.1" frames="00-01" />
		
		<Loop id="wideidle" path="wideidle" delay="0.1" frames="00" />
		<Loop id="widewalk" path="widewalk" delay="0.1" frames="00-01" />
		
		<Loop id="wingedidle" path="wingedidle" delay="0.1" frames="00" />
		<Loop id="winged" path="winged" delay="0.1" frames="00-01" />
		
		<Loop id="widewingedidle" path="widewingedidle" delay="0.1" frames="00" />
		<Loop id="widewinged" path="widewinged" delay="0.1" frames="00-01" />
		
		<!-- When this animation finishes playing, the goomba dies -->
		<Anim id="dead" path="dead" delay="0.1" frames="00"/>
		
    </koseiHelper_goomba>
	
